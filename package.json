    "prettier:write": "prettier --write '**/*.{js,ts,json}'",
    "prepare": "husky"
  },
  "dependencies": {
    "bull": "^4.12.2",
    "cors": "^2.8.5",
    "country-state-city": "^3.2.1",
    "csv-parse": "^5.5.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.5",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.2",
    "supertest": "^6.3.4",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/csv-parse": "^1.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.3",
    "@types/redis-mock": "^0.17.3",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "redis-mock": "^0.56.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
